{
	"name": "@lusc/truth-table",
	"version": "3.0.0",
	"repository": "https://github.com/melusc/truth-table",
	"author": "lusc",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"ava": {
		"files": [
			"test/*.test.ts"
		],
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"engines": {
		"node": "^16.9 || ^18 || ^20"
	},
	"files": [
		"dist/src",
		"*.md"
	],
	"scripts": {
		"fmt": "prettier -w . && xo --fix",
		"build": "rimraf dist && tsc",
		"test": "yarn run build && ava",
		"prepublish": "yarn build"
	},
	"devDependencies": {
		"@ava/typescript": "^4.0.0",
		"@lusc/tsconfig": "^2.2.0",
		"ava": "^5.3.0",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"typescript": "^5.0.4",
		"xo": "^0.54.2"
	},
	"packageManager": "yarn@3.5.1",
	"exports": {
		".": "./dist/src/index.js"
	},
	"dependencies": {
		"type-fest": "^3.11.0"
	}
}
