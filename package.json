{
	"name": "@lusc/truth-table",
	"version": "5.2.0",
	"repository": "https://github.com/melusc/truth-table",
	"author": "lusc",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"ava": {
		"files": [
			"test/**/*.test.ts"
		],
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"engines": {
		"node": "^18.17.1 || ^20.6.1 || ^22.0.0"
	},
	"files": [
		"dist/src",
		"*.md"
	],
	"scripts": {
		"fmt": "prettier -w . && xo --fix",
		"build": "rimraf dist && tsc",
		"test": "yarn run build && ava",
		"prepublish": "yarn build"
	},
	"devDependencies": {
		"@ava/typescript": "^4.1.0",
		"@lusc/tsconfig": "^4.0.0",
		"ava": "^6.1.2",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"typescript": "^5.4.2",
		"xo": "^0.58.0"
	},
	"packageManager": "yarn@4.1.1",
	"exports": {
		".": "./dist/src/index.js"
	},
	"dependencies": {
		"type-fest": "^4.12.0"
	}
}
